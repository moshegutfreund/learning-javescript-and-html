    <script>
      var fahrenheit, celcius;
      fahrenheit = Number(prompt("Enter degrees Fahrenheit: "));
      celcius = (fahrenheit - 32) * 5 / 9;
      alert ("Degrees Celcius = " + celcius);
    </script>
